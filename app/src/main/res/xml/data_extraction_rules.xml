<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude sensitive financial data from cloud backup -->
        <exclude domain="database" path="wealth_manager_database"/>
        <!-- Exclude encrypted user API keys from cloud backup -->
        <exclude domain="sharedpref" path="api_keys_prefs.xml"/>
    </cloud-backup>
    <device-transfer>
        <!-- Exclude sensitive data from device transfer -->
        <exclude domain="database" path="wealth_manager_database"/>
        <!-- Exclude encrypted user API keys from device transfer -->
        <exclude domain="sharedpref" path="api_keys_prefs.xml"/>
    </device-transfer>
</data-extraction-rules>